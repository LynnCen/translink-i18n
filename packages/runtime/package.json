{
  "name": "@translink/i18n-runtime",
  "version": "1.0.0",
  "description": "TransLink I18n Runtime - Lightweight, high-performance internationalization runtime library with Vue 3 and React support",
  "keywords": [
    "i18n",
    "runtime",
    "internationalization",
    "translation",
    "vue",
    "react",
    "cache"
  ],
  "author": "lynncen",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.esm.js",
      "require": "./dist/vue.js",
      "types": "./dist/vue.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "lru-cache": "^10.1.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "vitest": "^1.2.0",
    "vue": "^3.4.0",
    "react": "^18.2.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lynncen/translink-i18n.git",
    "directory": "packages/runtime"
  },
  "homepage": "https://github.com/lynncen/translink-i18n#readme",
  "bugs": {
    "url": "https://github.com/lynncen/translink-i18n/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
