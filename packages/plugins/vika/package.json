{
  "name": "@translink/plugin-vika",
  "version": "1.0.0",
  "description": "Vika integration plugin for TransLink I18n",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "translink",
    "i18n",
    "plugin",
    "vika",
    "translation"
  ],
  "author": "lynncen",
  "license": "MIT",
  "dependencies": {
    "@translink/i18n-cli": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "vitest": "^1.2.0",
    "@types/node": "^20.11.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

