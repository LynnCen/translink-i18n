{
  "name": "@translink/i18n-cli",
  "version": "1.0.0",
  "description": "TransLink I18n CLI Tool - Powerful internationalization management CLI with AI auto-translation",
  "keywords": [
    "i18n",
    "cli",
    "internationalization",
    "translation",
    "ast",
    "extract",
    "ai-translation",
    "excel",
    "automation"
  ],
  "author": "lynncen",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cli.js",
  "types": "./dist/cli.d.ts",
  "bin": {
    "translink": "./dist/cli.js",
    "tl": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@google/generative-ai": "^0.21.0",
    "commander": "^11.1.0",
    "exceljs": "^4.4.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "gogocode": "^1.0.0",
    "inquirer": "^9.2.0",
    "jiti": "^2.6.1",
    "openai": "^4.67.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/node": "^20.11.0",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.20.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lynncen/translink-i18n.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/lynncen/translink-i18n#readme",
  "bugs": {
    "url": "https://github.com/lynncen/translink-i18n/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
