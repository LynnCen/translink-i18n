{
  "name": "@translink/hash",
  "version": "1.0.0",
  "description": "TransLink Hash - Unified hash generation algorithm for CLI and Runtime",
  "keywords": [
    "i18n",
    "hash",
    "md5",
    "translink"
  ],
  "author": "lynncen",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "js-sha256": "^0.11.1"
  },
  "devDependencies": {
    "@types/md5": "^2.3.6",
    "@types/node": "^20.11.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lynncen/translink-i18n.git",
    "directory": "packages/hash"
  },
  "homepage": "https://github.com/lynncen/translink-i18n#readme",
  "bugs": {
    "url": "https://github.com/lynncen/translink-i18n/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
